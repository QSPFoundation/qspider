{
  "name": "qspider",
  "version": "1.3.1",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "nx": "nx",
    "start": "nx serve player",
    "start:standalone": "nx serve player-standalone",
    "start:desktop": "tauri dev",
    "start:desktop-standlone": "tauri dev -c src-tauri/standalone.conf.json",
    "start:docs": "nx serve docs",
    "serve:standalone-desktop": "nx run player-standalone:serve:desktop",
    "serve:desktop": "nx serve player-desktop",
    "prebuild": "rimraf ./dist",
    "build": "nx build --prod",
    "build:standalone": "nx build player-standalone --prod",
    "build:standalone-desktop": "nx run player-standalone:build:desktop --prod",
    "build:desktop": "nx build player-desktop --prod --emptyOutDir",
    "build:docs": "nx build docs --prod",
    "test": "nx run-many --all --target=test",
    "lint": "nx run-many --all --target=lint",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "release": "standard-version",
    "release:dry-run": "standard-version --dry-run",
    "workspace-generator": "nx workspace-generator",
    "prepare": "husky || true",
    "tauri": "tauri",
    "write-docs-translations": "nx write-translations docs --locale=ru && nx write-translations docs --locale=uk"
  },

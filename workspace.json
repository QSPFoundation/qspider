{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "defaultProject": "player",
  "projects": {
    "player": {
      "root": "apps/player",
      "sourceRoot": "apps/player/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "options": {
            "baseHref": "",
            "outputPath": "dist/apps/player",
            "index": "apps/player/src/index.html",
            "main": "apps/player/src/main.tsx",
            "polyfills": "apps/player/src/polyfills.ts",
            "tsConfig": "apps/player/tsconfig.app.json",
            "assets": [
              "apps/player/src/favicon.ico",
              "apps/player/src/assets",
              {
                "input": "./game",
                "glob": "**/*",
                "output": "./game"
              }
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "apps/player/webpack.config.js",
            "buildLibsFromSource": true
          },
          "configurations": {
            "production": {
              "assets": [
                "apps/player/src/favicon.ico",
                "apps/player/src/assets",
                {
                  "input": "./demo",
                  "glob": "**/*",
                  "output": "./"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/player/src/environments/environment.ts",
                  "with": "apps/player/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          },
          "outputs": ["{options.outputPath}"]
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "player:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "player:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["apps/player/tsconfig.app.json", "apps/player/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!apps/player/**/*"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/player/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/player"]
        }
      }
    },
    "player-desktop": {
      "root": "apps/player-desktop",
      "sourceRoot": "apps/player-desktop/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/player-desktop",
            "index": "apps/player-desktop/src/index.html",
            "main": "apps/player-desktop/src/main.tsx",
            "polyfills": "apps/player-desktop/src/polyfills.ts",
            "tsConfig": "apps/player-desktop/tsconfig.app.json",
            "assets": ["apps/player-desktop/src/favicon.ico", "apps/player-desktop/src/assets"],
            "styles": [],
            "scripts": [],
            "webpackConfig": "apps/player-desktop/webpack.config.js"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/player-desktop/src/environments/environment.ts",
                  "with": "apps/player-desktop/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "player-desktop:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "player-desktop:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/player-desktop/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "player-desktop-e2e": {
      "root": "apps/player-desktop-e2e",
      "sourceRoot": "apps/player-desktop-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/player-desktop-e2e/cypress.json",
            "devServerTarget": "player-desktop:serve",
            "tsConfig": "apps/player-desktop-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "player-desktop:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/player-desktop-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "player-e2e": {
      "root": "apps/player-e2e",
      "sourceRoot": "apps/player-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/player-e2e/cypress.json",
            "tsConfig": "apps/player-e2e/tsconfig.e2e.json",
            "devServerTarget": "player:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "player:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["apps/player-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/player-e2e/**/*"]
          }
        }
      }
    },
    "qsp-wasm": {
      "root": "libs/qsp-wasm",
      "sourceRoot": "libs/qsp-wasm/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["libs/qsp-wasm/tsconfig.lib.json", "libs/qsp-wasm/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!libs/qsp-wasm/**/*"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/qsp-wasm/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/libs/qsp-wasm"]
        },
        "compile": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "emcmake cmake .. && make"
              }
            ],
            "cwd": "libs/qsp-wasm/src/qsplib/build"
          }
        }
      }
    }
  },
  "generators": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "@emotion/styled"
      },
      "library": {
        "style": "@emotion/styled",
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    }
  }
}

{
  "version": 1,
  "projects": {
    "player": {
      "root": "apps/player",
      "sourceRoot": "apps/player/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/player",
            "index": "apps/player/src/index.html",
            "main": "apps/player/src/main.tsx",
            "polyfills": "apps/player/src/polyfills.ts",
            "tsConfig": "apps/player/tsconfig.app.json",
            "assets": [
              "apps/player/src/favicon.ico",
              "apps/player/src/assets",
              "apps/player/src/game"
            ],
            "styles": ["apps/player/src/styles.css"],
            "scripts": [],
            "webpackConfig": "apps/player/webpack.config.js"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/player/src/environments/environment.ts",
                  "with": "apps/player/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "player:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "player:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/player/.eslintrc",
            "tsConfig": [
              "apps/player/tsconfig.app.json",
              "apps/player/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/player/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/player/jest.config.js",
            "tsConfig": "apps/player/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "player-e2e": {
      "root": "apps/player-e2e",
      "sourceRoot": "apps/player-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/player-e2e/cypress.json",
            "tsConfig": "apps/player-e2e/tsconfig.e2e.json",
            "devServerTarget": "player:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "player:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/player-e2e/.eslintrc",
            "tsConfig": ["apps/player-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/player-e2e/**"]
          }
        }
      }
    },
    "qsp-wasm": {
      "root": "libs/qsp-wasm",
      "sourceRoot": "libs/qsp-wasm/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "libs/qsp-wasm/.eslintrc",
            "tsConfig": [
              "libs/qsp-wasm/tsconfig.lib.json",
              "libs/qsp-wasm/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/qsp-wasm/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/qsp-wasm/jest.config.js",
            "tsConfig": "libs/qsp-wasm/tsconfig.spec.json",
            "passWithNoTests": true
          }
        },
        "compile": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "../../tools/build-wasm.sh"
              }
            ],
            "cwd": "libs/qsp-wasm"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "player"
}
